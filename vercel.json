{
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':(exclude)cadence-ai-nextjs'",
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "suitegpt.app"
        }
      ],
      "destination": "/suitegpt.html"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.suitegpt.app"
        }
      ],
      "destination": "/suitegpt.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
