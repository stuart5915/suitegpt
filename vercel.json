{
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':(exclude)cadence-ai-nextjs'",
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "portfolio.suitegpt.app" }],
      "destination": "/portfolio/index.html"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/client-landing/index.html"
    },
    {
      "source": "/suitegpt-logo.png",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/suitegpt-logo.png"
    },
    {
      "source": "/suitegpt-logo.png",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/suitegpt-logo.png"
    },
    {
      "source": "/assets/:path*",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/assets/:path*"
    },
    {
      "source": "/assets/:path*",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/assets/:path*"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!trueform|opticrep|cheshbon|remcast-app|apps/).*)",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/suitegpt.html"
    },
    {
      "source": "/((?!trueform|opticrep|cheshbon|remcast-app|apps/).*)",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/suitegpt.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
