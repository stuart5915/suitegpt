{
    "cleanUrls": true,
    "redirects": [
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "www.getsuite.app" }],
            "destination": "https://getsuite.app/:path*",
            "permanent": true
        },
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "foodvitals.getsuite.app" }],
            "destination": "https://getsuite.app/foodvitals/:path*",
            "permanent": true
        }
    ]
}