{
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- . ':(exclude)cadence-ai-nextjs'",
  "redirects": [
    {
      "source": "/suite-shell.html",
      "has": [{ "type": "host", "value": "getsuite.app" }],
      "destination": "https://suitegpt.app/apps",
      "permanent": false
    },
    {
      "source": "/suite-shell.html",
      "has": [{ "type": "host", "value": "www.getsuite.app" }],
      "destination": "https://suitegpt.app/apps",
      "permanent": false
    },
    {
      "source": "/apps.html",
      "has": [{ "type": "host", "value": "getsuite.app" }],
      "destination": "https://suitegpt.app/apps",
      "permanent": false
    },
    {
      "source": "/apps.html",
      "has": [{ "type": "host", "value": "www.getsuite.app" }],
      "destination": "https://suitegpt.app/apps",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "portfolio.suitegpt.app" }],
      "destination": "/portfolio/index.html"
    },
    {
      "source": "/client-landing/:path*",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/client-landing/:path*"
    },
    {
      "source": "/assets/:path*",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/assets/:path*"
    },
    {
      "source": "/suitegpt-logo.png",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/suitegpt-logo.png"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "clients.suitegpt.app" }],
      "destination": "/client-landing/index.html"
    },
    {
      "source": "/suitegpt-logo.png",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/suitegpt-logo.png"
    },
    {
      "source": "/suitegpt-logo.png",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/suitegpt-logo.png"
    },
    {
      "source": "/assets/:path*",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/assets/:path*"
    },
    {
      "source": "/assets/:path*",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/assets/:path*"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/api/:path*",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!trueform|opticrep|cheshbon|remcast-app|docdigest|contractreader|resumeforge|emailcraft|essaycoach|copywriter|bizplan|socialpost|reviewresponder|legaldraft|codementor|mealplanner|quizmaker|languagebuddy|gymplan|historychat|sleepcoach|travelplanner|storyforge|adcopy|pethealth|physiobot|brandkit|pitchdeck|newsletter|symptommap|portfoliogpt|scienceexplainer|slideforge|nutriscan|tutorbot|plantdoctor|mealgenius|invoiceai|productdescriptor|mathsolver|homerepair|studycards|logoforge|s/).*)",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/suitegpt.html"
    },
    {
      "source": "/((?!trueform|opticrep|cheshbon|remcast-app|docdigest|contractreader|resumeforge|emailcraft|essaycoach|copywriter|bizplan|socialpost|reviewresponder|legaldraft|codementor|mealplanner|quizmaker|languagebuddy|gymplan|historychat|sleepcoach|travelplanner|storyforge|adcopy|pethealth|physiobot|brandkit|pitchdeck|newsletter|symptommap|portfoliogpt|scienceexplainer|slideforge|nutriscan|tutorbot|plantdoctor|mealgenius|invoiceai|productdescriptor|mathsolver|homerepair|studycards|logoforge|s/).*)",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/suitegpt.html"
    },
    {
      "source": "/s/:slug",
      "has": [{ "type": "host", "value": "suitegpt.app" }],
      "destination": "/api/serve-site?slug=:slug"
    },
    {
      "source": "/s/:slug",
      "has": [{ "type": "host", "value": "www.suitegpt.app" }],
      "destination": "/api/serve-site?slug=:slug"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/swarm/autonomy-loop",
      "schedule": "*/15 * * * *"
    }
  ]
}
