{
    "cleanUrls": true,
    "trailingSlash": true,
    "rewrites": [
        {
            "source": "/foodvitals",
            "destination": "/foodvitals/index.html"
        },
        {
            "source": "/notebox/",
            "destination": "/notebox.html"
        },
        {
            "source": "/notebox",
            "destination": "/notebox.html"
        },
        {
            "source": "/join/:code",
            "destination": "/join.html"
        },
        {
            "source": "/join/:code/",
            "destination": "/join.html"
        }
    ],
    "redirects": [
        {
            "source": "/:path*",
            "has": [{ "type": "host", "value": "foodvitals.getsuite.app" }],
            "destination": "https://www.getsuite.app/foodvitals/:path*",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/tg-app/(.*)",
            "headers": [
                { "key": "X-Content-Type-Options", "value": "nosniff" }
            ]
        },
        {
            "source": "/foodvitals/(.*)",
            "headers": [
                { "key": "X-Content-Type-Options", "value": "nosniff" }
            ]
        },
        {
            "source": "/foodvitals/manifest.json",
            "headers": [
                { "key": "Content-Type", "value": "application/manifest+json" }
            ]
        },
        {
            "source": "/foodvitals/sw.js",
            "headers": [
                { "key": "Service-Worker-Allowed", "value": "/foodvitals/" }
            ]
        }
    ]
}