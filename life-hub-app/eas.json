{
    "cli": {
        "version": ">= 5.2.0",
        "appVersionSource": "remote"
    },
    "build": {
        "base": {
            "node": "18.18.0",
            "env": {
                "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
            }
        },
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "channel": "preview"
        },
        "production": {
            "channel": "production",
            "autoIncrement": true
        }
    },
    "submit": {
        "production": {}
    }
}