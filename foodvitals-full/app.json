{
  "expo": {
    "name": "FoodVitals AI",
    "slug": "trueform-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "foodvitals",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.foodvitals.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "FoodVitals AI uses your camera to analyze food and track nutrition.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0A0A1A"
      },
      "package": "com.foodvitals.ai",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "FoodVitals AI",
      "shortName": "FoodVitals",
      "description": "Snap, scan & understand your nutrition instantly",
      "backgroundColor": "#0A0A1A",
      "themeColor": "#0A0A1A",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "FoodVitals AI uses your camera to analyze food and track nutrition."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a10df269-2575-4700-bb5e-1fcaaf4b53e8"
      }
    },
    "owner": "sgcnadesf"
  }
}
