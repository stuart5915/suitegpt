{
  "expo": {
    "name": "TrueForm AI",
    "slug": "trueform-ai",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trueform",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A2E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trueform.ai",
      "infoPlist": {
        "NSCameraUsageDescription": "TrueForm AI needs camera access to analyze your posture and movements in real-time.",
        "NSLocationWhenInUseUsageDescription": "TrueForm AI may use location to provide personalized recommendations based on your local climate and environment.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1A2E"
      },
      "package": "com.trueform.ai",
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "TrueForm AI",
      "shortName": "TrueForm",
      "description": "AI-powered posture and movement analysis",
      "backgroundColor": "#1A1A2E",
      "themeColor": "#1A1A2E",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "TrueForm AI needs camera access to analyze your posture and movements."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/trueform"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a10df269-2575-4700-bb5e-1fcaaf4b53e8"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/a10df269-2575-4700-bb5e-1fcaaf4b53e8"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}