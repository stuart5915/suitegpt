{
  "expo": {
    "name": "REMcast",
    "slug": "remcast",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "remcast",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0F23"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.remcast.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0F23"
      },
      "package": "com.remcast.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "description": "Your subconscious movie studio - transform dreams into AI-generated visual reels",
      "themeColor": "#6366F1",
      "backgroundColor": "#0F0F23",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-dev-client",
      "expo-sqlite",
      "@react-native-community/datetimepicker",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/remcast-app"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "618ff3fb-76ef-4710-bae3-86bd7b280c46"
      }
    }
  }
}
