{
  "expo": {
    "name": "OpticRep",
    "slug": "opticrep",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.opticrep.app",
      "infoPlist": {
        "NSCameraUsageDescription": "OpticRep needs camera access to track your form during workouts.",
        "NSMicrophoneUsageDescription": "OpticRep needs microphone access to record your workout reflections.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.opticrep.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png",
      "name": "OpticRep",
      "shortName": "OpticRep",
      "description": "AI-powered workout form tracking",
      "backgroundColor": "#000000",
      "themeColor": "#000000",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "scope": "/"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermissionText": "OpticRep needs camera access to track your form during workouts."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "OpticRep needs microphone access to record your workout reflections."
        }
      ],
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "opticrep",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c612c8ba-faa3-440a-8b62-9b00c1851c1a"
      }
    }
  }
}